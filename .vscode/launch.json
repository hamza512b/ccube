{
  "configurations": [
    {
      "name": "Debug file ./build/draw_cube",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/draw_cube",
      "args": ["50", "50"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Build with make"
    }
  ],
  "version": "2.0.0"
}
